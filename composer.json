{
    "name": "b2p/bdf-prime-mongodb",
    "description": "Bdf prime MongoDB component",
    "type": "library",
    "license": "proprietary",
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.b2pweb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bdf\\Prime\\MongoDB\\": "src/MongoDB"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "ext-mongodb": "*",
        "b2p/bdf-prime": "~1.3"
    },
    "require-dev": {
        "b2p/bdf-phpunit": "~1.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
